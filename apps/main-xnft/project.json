{
  "name": "main-xnft",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/main-xnft/src",
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "@nrwl/expo:start",
      "options": {
        "web": true
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx start main-xnft",
          "yarn xnft --iframe http://localhost:19006"
        ]
      }
    },
    "run-ios": {
      "executor": "@nrwl/expo:run",
      "options": {
        "platform": "ios"
      }
    },
    "run-android": {
      "executor": "@nrwl/expo:run",
      "options": {
        "platform": "android"
      }
    },
    "build": {
      "executor": "@nrwl/expo:build",
      "options": {}
    },
    "build-list": {
      "executor": "@nrwl/expo:build-list",
      "options": {}
    },
    "download": {
      "executor": "@nrwl/expo:download",
      "options": {
        "output": "apps/main-xnft/dist"
      }
    },
    "build-ios": {
      "executor": "@nrwl/expo:build-ios",
      "options": {}
    },
    "build-android": {
      "executor": "@nrwl/expo:build-android",
      "options": {}
    },
    "build-web": {
      "executor": "@nrwl/expo:build-web",
      "options": {}
    },
    "build-status": {
      "executor": "@nrwl/expo:build-web",
      "options": {}
    },
    "sync-deps": {
      "executor": "@nrwl/expo:sync-deps",
      "options": {}
    },
    "ensure-symlink": {
      "executor": "@nrwl/expo:ensure-symlink",
      "options": {}
    },
    "publish": {
      "executor": "@nrwl/expo:publish",
      "options": {}
    },
    "publish-set": {
      "executor": "@nrwl/expo:publish-set",
      "options": {}
    },
    "rollback": {
      "executor": "@nrwl/expo:rollback",
      "options": {}
    },
    "prebuild": {
      "executor": "@nrwl/expo:prebuild",
      "options": {}
    },
    "eject": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx prebuild main-xnft"
      }
    },
    "install": {
      "executor": "@nrwl/expo:install",
      "options": {}
    },
    "update": {
      "executor": "@nrwl/expo:update",
      "options": {}
    },
    "export": {
      "executor": "@nrwl/expo:export",
      "options": {
        "platform": "all",
        "outputDir": "../../dist/apps/main-xnft"
      }
    },
    "export-web": {
      "executor": "@nrwl/expo:export",
      "options": {
        "bundler": "metro"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/main-xnft/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/main-xnft/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
