{
  "name": "main-xnft",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/main-xnft/src",
  "projectType": "application",
  "targets": {
    "release": {
      "executor": "nx:run-commands",
      "options": {
        "command": "exit 0;"
      }
    },
    "start": {
      "executor": "@nx/expo:start",
      "options": {
        "web": true
      },
      "dependsOn": [
        "ensure-symlink",
        "sync-deps"
      ]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx start main-xnft",
          "yarn xnft --iframe http://localhost:19006"
        ]
      }
    },
    "run-ios": {
      "executor": "@nx/expo:run",
      "options": {
        "platform": "ios"
      },
      "dependsOn": [
        "ensure-symlink",
        "sync-deps"
      ]
    },
    "run-android": {
      "executor": "@nx/expo:run",
      "options": {
        "platform": "android"
      },
      "dependsOn": [
        "ensure-symlink",
        "sync-deps"
      ]
    },
    "build": {
      "executor": "@nx/expo:build",
      "options": {}
    },
    "build-list": {
      "executor": "@nx/expo:build-list",
      "options": {}
    },
    "download": {
      "executor": "@nx/expo:download",
      "options": {
        "output": "apps/main-xnft/dist"
      }
    },
    "sync-deps": {
      "executor": "@nx/expo:sync-deps",
      "options": {}
    },
    "ensure-symlink": {
      "executor": "@nx/expo:ensure-symlink",
      "options": {}
    },
    "prebuild": {
      "executor": "@nx/expo:prebuild",
      "options": {},
      "dependsOn": [
        "ensure-symlink",
        "sync-deps"
      ]
    },
    "install": {
      "executor": "@nx/expo:install",
      "options": {}
    },
    "update": {
      "executor": "@nx/expo:update",
      "options": {}
    },
    "export": {
      "executor": "@nx/expo:export",
      "options": {
        "platform": "all",
        "outputDir": "../../dist/apps/main-xnft"
      },
      "dependsOn": [
        "ensure-symlink",
        "sync-deps"
      ]
    },
    "export-web": {
      "executor": "@nx/expo:export",
      "options": {
        "bundler": "metro"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/main-xnft/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/main-xnft/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
